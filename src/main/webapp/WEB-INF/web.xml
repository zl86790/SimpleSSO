<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>name.lizhe.simplesso</display-name>
  <welcome-file-list>
    <welcome-file>/jsp/index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>name.lizhe.simplesso.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>AnotherServer</display-name>
    <servlet-name>AnotherServer</servlet-name>
    <servlet-class>name.lizhe.simplesso.controller.AnotherServer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AnotherServer</servlet-name>
    <url-pattern>/anotherServer</url-pattern>
  </servlet-mapping>
</web-app>